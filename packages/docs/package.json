{
  "name": "@canvas-ui/docs",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "dev": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@umijs/plugin-sass": "^1.1.1",
    "assert": "^2.0.0",
    "dumi": "^1.0.13",
    "immer": "^9.0.15",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "rxjs": "^7.5.6",
    "yorkie": "^2.0.0"
  }
}
